frr version 10.4.1
frr defaults traditional
hostname sa-ix-01
!
ip route 0.0.0.0/0 10.0.1.1 eth1
!
interface eth0
 description sa-noc-mgmt
 ip address 172.31.31.11/24
exit
!
interface eth1
 description sa-nat-gw
 ip address 10.0.1.101/24
exit
!
interface eth2
 description sa-core-01:eth1
 ip address 10.11.0.1/30
 ip ospf area 0
 ip ospf bfd
 ip ospf network point-to-point
exit
!
interface eth3
 description sa-core-02:eth1
 ip address 10.11.0.5/30
 ip ospf area 0
 ip ospf bfd
 ip ospf network point-to-point
exit
!
interface eth11
 description ae-ix-01:eth11
 ip address 172.16.0.1/30
exit
!
interface eth12
 description ae-ix-02:eth11
 ip address 172.16.0.5/30
exit
!
interface eth13
 description pk-ix-01:eth11
 ip address 172.16.0.17/30
exit
!
interface eth14
 description pk-ix-02:eth11
 ip address 172.16.0.21/30
exit
!
interface eth15
 description us-ix-01:eth11
 ip address 172.16.0.33/30
exit
!
interface eth16
 description us-ix-02:eth11
 ip address 172.16.0.37/30
exit
!
interface lo
 description sa-ix-01-lo
 ip address 10.255.11.1/32
 ip ospf area 0
 ip ospf bfd
exit
!
router bgp 65101
 bgp router-id 10.255.11.1
 no bgp ebgp-requires-policy
 bgp cluster-id 10.255.11.1
 timers bgp 30 90
 neighbor CLIENTS peer-group
 neighbor CLIENTS remote-as 65101
 neighbor CLIENTS update-source lo
 neighbor 10.255.11.3 peer-group CLIENTS
 neighbor 10.255.11.4 peer-group CLIENTS
 neighbor 10.255.11.5 peer-group CLIENTS
 neighbor 10.255.11.6 peer-group CLIENTS
 neighbor 10.255.11.7 peer-group CLIENTS
 neighbor 10.255.11.8 peer-group CLIENTS
 neighbor 10.255.11.9 peer-group CLIENTS
 neighbor 10.255.11.2 remote-as 65101
 neighbor 10.255.11.2 update-source lo
 neighbor 172.16.0.2 remote-as 65102
 neighbor 172.16.0.2 description ae-ix-01
 neighbor 172.16.0.6 remote-as 65102
 neighbor 172.16.0.6 description ae-ix-02
 neighbor 172.16.0.18 remote-as 65103
 neighbor 172.16.0.18 description pk-ix-01
 neighbor 172.16.0.22 remote-as 65103
 neighbor 172.16.0.22 description pk-ix-02
 neighbor 172.16.0.34 remote-as 65104
 neighbor 172.16.0.34 description us-ix-01
 neighbor 172.16.0.38 remote-as 65104
 neighbor 172.16.0.38 description us-ix-02
 !
 address-family ipv4 unicast
  neighbor CLIENTS route-reflector-client
  neighbor CLIENTS default-originate
 exit-address-family
exit
!
router ospf
 ospf router-id 10.255.11.1
 maximum-paths 1
exit
!
