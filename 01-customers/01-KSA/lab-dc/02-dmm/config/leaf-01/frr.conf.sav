frr version 10.4.1
frr defaults traditional
hostname dmm-l1
!
interface eth1
 description dmm-dc-sp1:eth2
 ip address 10.121.0.10/30
exit
!
interface eth2
 description dmm-dc-sp2:eth2
 ip address 10.121.0.14/30
exit
!
interface lo
 description dmm-lab-dc-lo-11
 ip address 10.255.121.3/32
exit
!
router bgp 65121
 bgp router-id 10.255.121.3
 no bgp ebgp-requires-policy
 bgp bestpath as-path multipath-relax
 neighbor 10.121.0.9 remote-as 65060
 neighbor 10.121.0.9 description dmm-sp1
 neighbor 10.121.0.13 remote-as 65060
 neighbor 10.121.0.13 description dmm-sp2
 !
 address-family ipv4 unicast
  network 10.121.0.8/30
  network 10.121.0.12/30
  network 10.255.121.3/32
  maximum-paths 2
 exit-address-family
exit
!
