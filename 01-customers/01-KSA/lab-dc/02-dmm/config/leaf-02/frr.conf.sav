frr version 10.4.1
frr defaults traditional
hostname dmm-l2
!
interface eth1
 description dmm-dc-sp1:eth3
 ip address 10.121.0.18/30
exit
!
interface eth2
 description dmm-dc-sp2:eth3
 ip address 10.121.0.22/30
exit
!
interface lo
 description dmm-lab-dc-lo-12
 ip address 10.255.121.4/32
exit
!
router bgp 65122
 bgp router-id 10.255.121.4
 no bgp ebgp-requires-policy
 bgp bestpath as-path multipath-relax
 neighbor 10.121.0.17 remote-as 65060
 neighbor 10.121.0.17 description dmm-sp1
 neighbor 10.121.0.21 remote-as 65060
 neighbor 10.121.0.21 description dmm-sp2
 !
 address-family ipv4 unicast
  network 10.121.0.16/30
  network 10.121.0.20/30
  network 10.255.121.4/32
  maximum-paths 2
 exit-address-family
exit
!
