frr version 10.4.1
frr defaults traditional
hostname dmm-sp1
!
interface eth1
 description dmm-dc-lab-ce-01:eth2
 ip address 10.121.0.2/30
exit
!
interface eth2
 description dmm-dc-l1:eth1
 ip address 10.121.0.9/30
exit
!
interface eth3
 description dmm-dc-l2:eth1
 ip address 10.121.0.17/30
exit
!
interface eth4
 description dmm-dc-l3:eth1
 ip address 10.121.0.25/30
exit
!
interface lo
 description dmm-lab-dc-lo-1
 ip address 10.255.121.1/32
exit
!
router bgp 65060
 bgp router-id 10.255.121.1
 no bgp ebgp-requires-policy
 neighbor 10.121.0.1 remote-as 65200
 neighbor 10.121.0.1 description dmm-ce-01
 neighbor 10.121.0.10 remote-as 65121
 neighbor 10.121.0.10 description dmm-l1
 neighbor 10.121.0.18 remote-as 65122
 neighbor 10.121.0.18 description dmm-l2
 neighbor 10.121.0.26 remote-as 65123
 neighbor 10.121.0.26 description dmm-l3
 !
 address-family ipv4 unicast
  network 10.121.0.0/30
  network 10.121.0.8/30
  network 10.121.0.16/30
  network 10.121.0.24/30
  network 10.255.121.1/32
 exit-address-family
exit
!
