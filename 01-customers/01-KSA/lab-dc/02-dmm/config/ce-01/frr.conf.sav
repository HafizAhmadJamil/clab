frr version 10.4.1
frr defaults traditional
hostname dmm-ce-01
!
ip route 0.0.0.0/0 10.0.121.1
!
interface eth1
 description sa-dmm-01:eth3
 ip address 10.0.121.2/30
exit
!
interface eth2
 description dmm-dc-sp1:eth1
 ip address 10.121.0.1/30
exit
!
interface eth3
 description dmm-dc-sp2:eth1
 ip address 10.121.0.5/30
exit
!
interface lo
 ip address 203.0.113.2/32
exit
!
interface lo1
 description dmm-lab-dc-lo-10
 ip address 10.255.121.0/32
exit
!
router bgp 65200
 bgp router-id 10.255.121.0
 no bgp ebgp-requires-policy
 neighbor 10.121.0.2 remote-as 65060
 neighbor 10.121.0.2 description dmm-sp1
 neighbor 10.121.0.6 remote-as 65060
 neighbor 10.121.0.6 description dmm-sp2
 !
 address-family ipv4 unicast
  network 0.0.0.0/0
  network 10.121.0.0/30
  network 10.121.0.4/30
  network 10.255.121.0/32
 exit-address-family
exit
!
