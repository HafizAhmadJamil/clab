frr version 10.4.1
frr defaults traditional
hostname dmm-sp2
!
interface eth1
 description dmm-dc-lab-ce-01:eth3
 ip address 10.121.0.6/30
exit
!
interface eth2
 description dmm-dc-l1:eth2
 ip address 10.121.0.13/30
exit
!
interface eth3
 description dmm-dc-l2:eth2
 ip address 10.121.0.21/30
exit
!
interface eth4
 description dmm-dc-l3:eth2
 ip address 10.121.0.29/30
exit
!
interface lo
 description dmm-lab-dc-lo-2
 ip address 10.255.121.2/32
exit
!
router bgp 65060
 bgp router-id 10.255.121.2
 no bgp ebgp-requires-policy
 neighbor 10.121.0.5 remote-as 65200
 neighbor 10.121.0.5 description dmm-ce-01
 neighbor 10.121.0.14 remote-as 65121
 neighbor 10.121.0.14 description dmm-l1
 neighbor 10.121.0.22 remote-as 65122
 neighbor 10.121.0.22 description dmm-l2
 neighbor 10.121.0.30 remote-as 65123
 neighbor 10.121.0.30 description dmm-l3
 !
 address-family ipv4 unicast
  network 10.121.0.4/30
  network 10.121.0.12/30
  network 10.121.0.20/30
  network 10.121.0.28/30
  network 10.255.121.2/32
 exit-address-family
exit
!
